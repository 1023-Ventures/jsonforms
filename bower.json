{
  "name": "jsonForms",
  "description": "JSONForms is an AngularJS-based framework to simplify the creation of forms for data entry and edit in web applications. It allows to declaratively define data and layout of a form and to embed the form into your HTML with one simple <jsonforms> tag.",
  "version": "0.0.1",
  "homepage": "https://github.com/qb-project/jsonforms",
  "license": "MIT",
  "main": [
    "dist/js/jsonforms.js",
    "dist/css/jsonforms.css"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "tests"
  ],
  "dependencies": {
    "angular": "1.2.x",
    "angular-route": "1.2.x",
    "angular-loader": "1.2.x",
    "angular-mocks": "~1.2.x",
    "angular-ui": "0.4.0",
    "angular-ui-ace": "0.2.3",
    "angular-ui-grid": "3.0.0-rc.21",
    "angular-ui-bootstrap-bower": "0.11.2",
    "bootstrap": "3.3.4",
    "ui-utils": "0.1.1",
    "jquery": "1.11.x",
    "json-refs": "~0.2.0",
    "angular-resource": "~1.2.x"
  }
}
